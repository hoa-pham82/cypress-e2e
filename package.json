{
	"name": "cypress-api",
	"version": "1.0.0",
	"description": "",
	"main": "cypress.config.js",
	"scripts": {
		"cy:open": "cypress open",
		"cy:run": "npm run clean-report && cypress run && npm run merge-build-report",
		"cy:run-first": "cypress run",
		"merge-report": "mochawesome-merge \"cypress/results/*.json\" > mochawesome.json",
		"build-report": "marge mochawesome.json",
		"merge-build-report": "npm run merge-report && npm run build-report",
		"clean-report": "rmdir /s /q \"cypress/results/",
		"cy:run:get": "cypress run --spec cypress/e2e/API/petStore/GET_Pet_Status.test.js --env baseUrl=https://petstore.swagger.io/v2/pet",
		"cy:run:post": "cypress run --spec cypress/e2e/API/petStore/POST_Add_New_Pet.test.js --env baseUrl=https://petstore.swagger.io/v2/pet",
		"cy:run:put": "cypress run --spec cypress/e2e/API/petStore/PUT_Update_Pet.test.js --env baseUrl=https://petstore.swagger.io/v2/pet"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"cypress": "^12.7.0",
		"marge": "^1.0.1",
		"mochawesome": "^7.1.3",
		"mochawesome-merge": "^4.2.2"
	}
}
